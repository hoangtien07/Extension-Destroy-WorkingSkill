{
    "name": "Destroy WorkingSkill Extension",
    "manifest_version": 3,
    "version": "1.0.0",
    "description": "Extension for WokrkingSkill",
    "icons": {
        "16": "./images/favicon-16x16.png",
        "32": "./images/favicon-32x32.png"
    },
    "action": {
        "default_title": "Destroy WorkingSkill Extension",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "https://www.workingskills.net/courses/*/lessons/*"
        ],
        "js": [
            "./content.js"
        ]
    }]
}